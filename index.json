    "license": "MIT",
    "tags": [
      "UI",
      "ダークモード化"
    ],
    "dependencies": [],
    "images": [
      {
        "src": "https://picsum.photos/seed/b/800/400",
        "alt": "screenshot2"
      },
      {
        "src": "https://picsum.photos/seed/c/800/400",
        "alt": "screenshot3"
      }
    ],
    "installer": {
      "source": {
        "github": {
          "owner": "hebiiro",
          "repo": "al2_jd",
          "pattern": "^al2_jd_r\\d+\\.zip$"
        }
      },
      "install": [
        {
          "action": "download"
        },
        {
          "action": "extract",
          "to": "{tmp}"
        },
        {
          "action": "copy",
          "from": "{tmp}/Plugin/*",
          "to": "{pluginsDir}"
        }
      ]
    },
    "version": [
      {
        "version": "r15",
        "release_date": "2024-12-15",
