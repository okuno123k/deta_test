  {
    "id": "Someone.InputPluginX",
    "name": "Input Plugin X",
    "type": "入力",
    "summary": "各種フォーマットの入力をサポート",
    "description": "MP4, MKV などの入力をサポートするプラグイン。",
    "author": "Someone",
    "latest-version": "1.2.0",
    "repoURL": "https://example.com/repo2",
    "downloadURL": "https://example.com/inputx.zip",
    "license": "Apache-2.0",
    "tags": ["映像", "コーデック"],
    "dependencies": ["AviUtlExEdit2"],
    "installation": "plugins/input へ配置してください。",
    "images": [
      { "src": "https://picsum.photos/seed/b/800/400", "alt": "screenshot2" },
      { "src": "https://picsum.photos/seed/c/800/400", "alt": "screenshot3" }
    ],
    "version": [
      {
        "version": "1.2.0",
        "release_date": "2024-12-15",
        "file": []
      }
    ],
    "installer": {
      "source": { "direct": "https://example.com/inputx.zip" },
      "install": [
        { "action": "download", "to": "{tmp}/pkg.zip" },
        { "action": "extract", "from": "{tmp}/pkg.zip", "to": "{tmp}/pkg" },
        { "action": "ensureDirs", "paths": ["{pluginsDir}"] },
        { "action": "copy", "from": "{tmp}/pkg/*.auf", "to": "{pluginsDir}" }
      ],
      "uninstall": [
        { "action": "delete", "path": "{pluginsDir}/plugin.auf" }
      ]
    }
  },
