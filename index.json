  {
    "id": "hebiiro.al2_jd",
    "name": "ダークモード化プラグイン",
    "type": "入力プラグイン",
    "summary": "aviutl2を完全にダークモード化することを目的としたMODです。",
    "description": "AviUtl2のUIをダークモードに変更するプラグインです。",
    "author": "hebiiro",
    "repoURL": "https://github.com/hebiiro/al2_jd/",
    "license": "MIT",
    "tags": [
      "UI",
      "ダークモード化"
    ],
    "dependencies": [],
    "images": [
      {
        "src": "https://picsum.photos/seed/b/800/400",
        "alt": "screenshot2"
      },
      {
        "src": "https://picsum.photos/seed/c/800/400",
        "alt": "screenshot3"
      }
    ],
    "installer": {
      "source": {
        "github": {
          "owner": "hebiiro",
          "repo": "al2_jd",
          "pattern": "^al2_jd_r\\d+\\.zip$"
        }
      },
      "install": [
        {
          "action": "download"
        },
        {
          "action": "extract",
          "to": "{tmp}"
        },
        {
          "action": "copy",
          "from": "{tmp}/Plugin/*",
          "to": "{pluginsDir}"
        }
      ],
      "uninstall": [
        {
          "action": "delete",
          "path": "{pluginsDir}/al2_jd.aui2"
        },
        {
          "action": "delete",
          "path": "{pluginsDir}/DarkenWindow.aul"
        },
        {
          "action": "delete",
          "path": "{pluginsDir}/al2/"
        }
      ]
    },
    "version": [
      {
        "version": "r1",
        "release_date": "2025-07-24",
